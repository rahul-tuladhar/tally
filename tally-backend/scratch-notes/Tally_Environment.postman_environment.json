{
  "id": "tally-backend-env",
  "name": "Tally Backend Environment",
  "values": [
    {
      "key": "base_url",
      "value": "http://localhost:8000",
      "description": "Base URL for the Tally backend API",
      "type": "default",
      "enabled": true
    },
    {
      "key": "api_version",
      "value": "v1",
      "description": "API version",
      "type": "default",
      "enabled": true
    },
    {
      "key": "control_id",
      "value": "",
      "description": "Control ID for testing (auto-populated from responses)",
      "type": "default",
      "enabled": true
    },
    {
      "key": "document_id",
      "value": "",
      "description": "Document ID for testing (auto-populated from responses)",
      "type": "default",
      "enabled": true
    },
    {
      "key": "ai_response_id",
      "value": "",
      "description": "AI Response ID for testing (auto-populated from responses)",
      "type": "default",
      "enabled": true
    },
    {
      "key": "openai_api_key",
      "value": "your-openai-api-key",
      "description": "OpenAI API Key for AI processing",
      "type": "secret",
      "enabled": true
    },
    {
      "key": "reducto_api_key",
      "value": "your-reducto-api-key",
      "description": "Reducto API Key for document processing",
      "type": "secret",
      "enabled": true
    },
    {
      "key": "test_file_path",
      "value": "/path/to/test/document.pdf",
      "description": "Path to test document for upload testing",
      "type": "default",
      "enabled": true
    }
  ],
  "_postman_variable_scope": "environment"
} 